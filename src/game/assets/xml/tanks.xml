</tk>
<tk id='1014' icon='AnnoT49' lv='Ⅷ' lock='10' type='l' name='T49' hp='1000' chuan='250' attack='320' shoot_rate='10' armor='25' move_speed='72'>
<req pao='100' pao_lv='1' ta='100' ta_lv='1' fdj='200' fdj_lv='1' dai='200' dai_lv='1' money='2000' rate='10'/>
